doctype 5
html
	head
		title= title
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		link(href='http://fonts.googleapis.com/css?family=Andika', rel='stylesheet', type='text/css')
		link(rel='stylesheet', href='/stylesheets/bootstrap.min.css', type='text/css', media='screen')
		link(rel='stylesheet', href='/stylesheets/bootstrap-responsive.min.css', type='text/css', media='screen')
		link(rel='stylesheet', href='/stylesheets/styles.css', type='text/css', media='screen')
		script(type='text/javascript', src='/javascripts/lib/jquery-1.7.2.min.js')
		script(type='text/javascript', src='/javascripts/oyw.js')
		script(type='text/javascript', src='/javascripts/lib/marked.js')
		
	body
		.container-fluid.row-fluid			
			block header
				div#header.span12
					h1= title
			block navigation
				div#nav.span3	
						ul
							li
								a(href="#") About
			div#main
				block content				
				block comment
			block footer
				div#footer.span12
					span Find me on 
					a(href='#') Google+ 
					, 
					a(href='#') GitHub 
					, 
					a(href='#') Twitter

